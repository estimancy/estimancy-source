$(".component_tree").html("<%=raw j generate_wbs_product(@project.root_component, @project, '', 5) %>");

$(".input_data").html("<%=raw j(display_input) %>");

$(".output_data").html("<%=raw j(display_results) %>");

$(".component_name").html("<%=raw current_component %>");

$('.component_tree > ul li').hover(
    function () {
      $(this.children).css('display', 'inline');
    },
    function () {
     $('.block_link').hide();
    }
);

$('.spiner').hide();

$("#pbs_list").val(<%= current_component.id %>)

