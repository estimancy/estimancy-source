<div class="span12">
  <%= simple_form_for(@admin_setting) do |f| %>

    <h1><%= controller.controller_name.humanize.singularize %> </h1>

    <div >
      <%= f.input :key %>

      <%= f.input :value %>
    </div>
    <div class="actions">
      <%= f.submit "Save", :class => "btn btn-success" %>
      <%= f.submit "Apply", :class => "btn btn-success" %>
    </div>
  <% end %>
</div>