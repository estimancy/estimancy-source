<h1>Listing organization_labor_categories</h1>

<table class="table table-striped">
  <tr>
    <th><%= I18n.t(:organization) %></th>
    <th><%= I18n.t(:labor_category) %></th>
    <th><%= I18n.t(:level) %></th>
    <th><%= I18n.t(:name) %></th>
    <th><%= I18n.t(:description) %></th>
    <th><%= I18n.t(:cost_per_hour) %></th>
    <th><%= I18n.t(:base_year) %></th>
    <th><%= I18n.t(:currency) %></th>
    <th><%= I18n.t(:hour_per_day) %></th>
    <th><%= I18n.t(:days_per_year) %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @organization_labor_categories.each do |organization_labor_category| %>
  <tr>
    <td><%= organization_labor_category.organization_id %></td>
    <td><%= organization_labor_category.labor_category_id %></td>
    <td><%= organization_labor_category.level %></td>
    <td><%= organization_labor_category.name %></td>
    <td><%= organization_labor_category.description %></td>
    <td><%= organization_labor_category.cost_per_hour %></td>
    <td><%= organization_labor_category.base_year %></td>
    <td><%= organization_labor_category.currency_id %></td>
    <td><%= organization_labor_category.hour_per_day %></td>
    <td><%= organization_labor_category.days_per_year %></td>
    <td><%= link_to '', edit_organization_labor_category_path(organization_labor_category), :class => 'icn_edit', :title => I18n.t('edit') %></td>
    <td><%= link_to '', organization_labor_category, confirm: I18n.t('are_you_sur'), method: :delete, :class => 'icn_trash', :title =>I18n.t('delete') %></td>
  </tr>
<% end %>
</table>

<br />

