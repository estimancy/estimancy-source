<%= simple_form_for(@abacus_organization) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :value %>
    <%= f.input :unit_of_work_id %>
    <%= f.input :organization_uow_complexity_id %>
    <%= f.input :organization_technology_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
