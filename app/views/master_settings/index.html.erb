<div class="span12">
  <h1>Master Setting</h1>

  <table class="table table-striped">
    <tr>
      <th>Key</th>
      <th>Value</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>

  <% @master_settings.each do |master_setting| %>
    <tr >
      <td><%= master_setting.key %></td>
      <td><%= master_setting.value %></td>
      <td><%= link_to 'Show', master_setting %></td>
      <td><%= link_to 'Edit', edit_master_setting_path(master_setting) %></td>
      <td><%= link_to 'Destroy', master_setting, confirm: 'Are you sure?', method: :delete %></td>
      <% if master_setting.is_proposed? %>
          <td><%= link_to('Validate', "master_settings/#{master_setting.id}/validate_change", confirm: 'Do you confirm changes validation on this record?', :title => "validate changes")%></td>
      <% end %>
    </tr>
  <% end %>
  </table>
  <%= link_to 'New Master setting', new_master_setting_path, :class => "btn" %>
</div>