<%= simple_form_for(@wbs_project_element) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :wbs_activity_element_id %>
    <%= f.input :wbs_activity_id %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :additional_description %>
    <%= f.input :exclude %>
    <%= f.input :ancestry %>
    <%= f.input :owner_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
