<%= form_tag notes_form_path(params[:factor_id]), method: "POST" do %>
  <div class="span12">

    <div class="zdzd">
      <label>Notes</label>
      <%= text_area_tag "notes", @notes, class: "attribute_note_area" %>
    </div>

    <br>

    <%= submit_tag "Save", class: "btn" %>
  </div>
<% end %>