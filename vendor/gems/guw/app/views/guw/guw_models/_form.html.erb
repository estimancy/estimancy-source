<%= simple_form_for(@guw_model) do |f| %>
  <%= f.error_notification %>

  <%= f.input :organization_id, as: :hidden, input_html: { value: params[:organization_id] } %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>