<div class="tabs">

  <ul>
    <li>
      <a href="#tabs-1">
        Attributs
      </a>
    </li>
  </ul>

  <div id="tabs-1">
    <%= simple_form_for(@guw_attribute_complexity) do |f| %>
      <%= f.error_notification %>

      <div class="form-inputs">
        <%= f.input :enable_value %>
        <%= f.input :bottom_range %>
        <%= f.input :top_range %>
      </div>

      <% if can? :manage_modules_instances, ModuleProject %>
        <%= f.button :submit, I18n.t('save')  %>
      <% end %>
      <%= link_to I18n.t(:back), :back, class: "btn" %>
    <% end %>
  </div>
</div>