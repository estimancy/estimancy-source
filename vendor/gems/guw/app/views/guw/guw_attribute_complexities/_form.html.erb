<%= simple_form_for(@guw_attribute_complexity) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :guw_type_id, as: :hidden, input_html: { value: params[:guw_type_id] } %>

    <%= f.input :name %>
    <%= f.input :value %>
    <%= f.input :bottom_range %>
    <%= f.input :top_range %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
    <%= link_to I18n.t(:back), :back, class: "btn" %>
  </div>
<% end %>