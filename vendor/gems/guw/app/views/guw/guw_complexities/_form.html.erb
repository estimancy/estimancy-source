<div class="tabs">

  <ul>
    <li>
      <a href="#tabs-1">
        Complexities
      </a>
    </li>
  </ul>

  <div id="tabs-1">
    <%= simple_form_for(@guw_complexity) do |f| %>
      <%= f.error_notification %>

      <div class="form-inputs">

        <%= f.input :guw_type_id, as: :hidden, input_html: { value: params[:guw_type_id] } %>

        <%= f.input :name %>
        <%= f.input :alias %>
        <% if @guw_model.one_level_model == false %>
          <%= f.input :bottom_range %>
          <%= f.input :top_range %>
        <% end %>
      </div>

      <% if can? :manage_modules_instances, ModuleProject %>
        <%= f.button :submit, I18n.t('save') %>
      <% end %>
        <%= link_to I18n.t(:back), :back, class: "btn" %>
    <% end %>
  </div>
</div>