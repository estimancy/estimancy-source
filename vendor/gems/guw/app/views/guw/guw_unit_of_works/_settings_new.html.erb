<%= form_tag guw.guw_unit_of_works_save_guw_unit_of_works_with_multiple_outputs_path, id: "guw_form" do %>

  <div class="modal fade" id="modal_deported" tabindex="-1" role="dialog" aria-labelledby="modal_deported_label" aria-hidden="true" backdrop="static">
    <div class="modal-dialog">
      <div class="modal-deported" id="modal_deported_content">
      </div>
    </div>
  </div>

  <!-- Vue  ModuleProjectGuwUnitOfWorkGroup -->
  <%# @group_unit_of_works = Guw::GuwUnitOfWorkGroup.where(pbs_project_element_id: current_component.id, module_project_id: current_module_project.id) %>
  <%# @group_unit_of_works = ModuleProjectGuwUnitOfWorkGroup.where(organization_id: @guw_model.organization_id, project_id: @project.id,
                                                                  module_project_id: current_module_project.id, pbs_project_element_id: current_component.id) %>
  <% @guw_coefficients = @guw_model.guw_coefficients %>

  <div class="row-fluid">
    <div class="span12">
      <% if can? :execute_estimation_plan, @project %>
        <%= link_to I18n.t(:manage_the_groups), guw.guw_model_guw_unit_of_work_groups_path(@guw_model), class: "btn" %>

      <% end %>

      <% unless @guw_model.allow_excel == false && @guw_model.allow_jira == false && @guw_model.allow_redmine == false && @guw_model.allow_ml == false %>
        <button type="button" class="btn" data-toggle="modal" data-target="#extract">
          <%= I18n.t(:import) %>
        </button>
      <% end %>

      <%= link_to I18n.t(:export), guw.guw_model_export_path(@guw_model), class: "btn", method: "post" %>

      <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#stats_modal">
        Statistiques détaillées
      </button>

      <% if @guw_model.view_data == true %>
        <%= link_to "Visualiser les données", guw.guw_model_show_data_model_path(@guw_model, component_id: current_component.id, module_project_id: current_module_project.id), method: "get", class: "btn btn-primary", target: :_blank %>
      <% end %>

      <%= link_to "RAZ", "#!", class: "btn btn-inverse reset_retained pull-right button_margin_right", onclick: "multiraz()" %>
    </div>
  </div>

  <div class="row-fluid">
    <hr>
    <div class="pull-right">
      <% @guw_outputs = @guw_model.guw_outputs %>
      <% @guw_outputs.where(allow_subtotal: true).each do |guw_output| %>
        <span class="label label-success">
          Total <%= guw_output.name %> retenu :
          <span class="ajusted_size">
            <!-- Vue  ModuleProjectGuwUnitOfWork -->
            <%= @unit_of_work_groups.map do |i|
              #i.guw_unit_of_works.where(selected: true).flatten.map do |i|
              i.module_project_guw_unit_of_works.where(selected: true).flatten.map do |i|
                i.ajusted_size.is_a?(Numeric) ? i.ajusted_size.to_f : (i.ajusted_size.nil? ? 0 : i.ajusted_size["#{guw_output.id}"].to_f)
              end
            end.flatten.compact.sum.to_f.round(3) %>
          </span>
        </span>
      <% end %>

      <% @guw_outputs.where(allow_subtotal: true).each do |guw_output| %>
        <span class="label label-success">
          Total <%= guw_output.name %> théorique :
          <span class="ajusted_size">
            <!-- Vue  ModuleProjectGuwUnitOfWork -->
            <%= @unit_of_work_groups.map do |i|
              #i.guw_unit_of_works.where(selected: true).flatten.map do |i|
              i.module_project_guw_unit_of_works.where(selected: true).flatten.map do |i|
                i.size.is_a?(Numeric) ? i.size.to_f : (i.size.nil? ? 0 : i.size["#{guw_output.id}"].to_f)
              end
            end.flatten.compact.sum.to_f.round(3) %>
          </span>
        </span>
      <% end %>

      <!-- Vue  ModuleProjectGuwUnitOfWork et ModuleProjectGuwUnitOfWorkGoup -->
      <span class="label label-info">
        <%= I18n.t(:number_of_uw_selected_lines) %> :
        <span class="selected_of_unit_of_work">
          <%= "#{@unit_of_work_groups.map(&:number_of_uow_selected_lines).sum} / #{@unit_of_work_groups.map(&:number_of_uow_lines).sum}" %>
        </span>
      </span>
    </div>
  </div>

  <hr>

  <div class="accordion" id="accordion">
    <% if @unit_of_work_groups.empty? %>
      <% if can? :execute_estimation_plan, @project %>
        <%= link_to I18n.t('add_new_unit_of_work_group'), guw.new_guw_unit_of_work_group_path, class: "btn pull-left button_margin_right" %>
        <% unless @unit_of_work_groups.empty? %>
          <%= link_to I18n.t(:new_unit_of_work), guw.new_guw_model_guw_unit_of_work_path(@guw_model, g: uowg), class: "btn pull-left button_margin_right" %>
        <% end %>
      <% end %>
    <% end %>

    <% @unit_of_work_groups.each_with_index do |uowg, j| %>

      <%# uowg_guw_unit_of_works = uowg.guw_unit_of_works %>
      <% uowg_guw_unit_of_works = uowg.module_project_guw_unit_of_works %>

      <div class="accordion-heading" style="padding:5px;">
        <div class="row-fluid">
          <div class="span3">
            <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapse<%=uowg.id%>" id="accordion<%=uowg.id%>">
            <span style="font-weight: bold; font-size: 20px;" title="<%= uowg.comments %>">
              <%= uowg.name %>
            </span>
            </a>
          </div>

          <div class="span9">
            <% if can?(:execute_estimation_plan, @project) %>
              <%= link_to '', guw.edit_guw_model_guw_unit_of_work_group_path(@guw_model, uowg), class: "fa fa-pencil pull-right", style: "margin-top:3px;" %>
            <% end %>

            <div class="pull-right">

              <% @guw_outputs.where(allow_subtotal: true).each do |guw_output| %>
                <span class="label label-success">
                  Total <%= guw_output.name %> R. :
                  <span>
                    <%= [
                          uowg_guw_unit_of_works.where(selected: true).flatten.map do |i|
                            i.ajusted_size.is_a?(Numeric) ? i.ajusted_size.to_f : (i.ajusted_size.nil? ? 0 : i.ajusted_size["#{guw_output.id}"].to_f)
                          end
                        ].flatten.compact.sum.to_f.round(3) %>
                  </span>
                </span>
              <% end %>

              <% @guw_outputs.where(allow_subtotal: true).each do |guw_output| %>
                <span class="label label-success">
                  Total <%= guw_output.name %> T. :
                  <span>
                    <%= [
                          uowg_guw_unit_of_works.where(selected: true).flatten.map do |i|
                            i.size.is_a?(Numeric) ? i.size.to_f : (i.size.nil? ? 0 : i.size["#{guw_output.id}"].to_f)
                          end
                        ].flatten.compact.sum.to_f.round(3) %>
                  </span>
                </span>
              <% end %>

              <span class="label label-info">
                <%= I18n.t(:number_of_uw_selected) %> :
                <span class="<%= uowg.id %>_selected">
                  <%#= uowg_guw_unit_of_works.where(selected: true).map(&:quantity).compact.sum %>
                  <%= "#{uowg.number_of_uow_selected_lines} / #{uowg.number_of_uow_lines}" %>
                </span>
              </span>
            </div>
          </div>
        </div>
      </div>

      <div id="collapse<%= uowg.id %>" class="accordion-body collapse in">
        <% @guw_types = @guw_model.guw_types.includes(:guw_complexities) %>

        <div class="accordion-inner">
          <table class="table table-bordered table-condensed" id="guw">
            <tr>
              <th></th>
              <th>#</th>
              <th><%= I18n.t :name %></th>
              <th>UO</th>

              <% @guw_model.orders.sort_by { |k, v| v.to_f }.each do |i| %>
                <% unless i[1].blank? %>
                  <% if Guw::GuwCoefficient.where(name: i[0]).first.class == Guw::GuwCoefficient %>
                    <% guw_coefficient = Guw::GuwCoefficient.where(name: i[0], guw_model_id: @guw_model.id, deported: false).first %>
                    <% unless guw_coefficient.nil? %>
                      <th>
                        <%= i[0] %>
                      </th>
                    <% end %>
                  <% elsif i[0] == "Coefficient" %>
                    <% unless @guw_model.guw_coefficients.where(deported: true).map(&:id).empty? %>
                      <th>
                        <%= i[0] %>
                      </th>
                    <% end %>
                  <% elsif i[0] == "Critères" %>
                    <% if @guw_types.map{ |i| i.allow_criteria }.flatten.include?(true) %>
                      <th>
                        <%= I18n.t :criteria %>
                      </th>
                    <% end %>
                  <% elsif i[0] == "Coeff. de Complexité" %>
                    <% if i[1].blank? %>
                    <% else %>
                      <th>
                        <% if @guw_types.map{|i| i.allow_criteria }.flatten.include?(true) %>
                          Coeff. de complexité
                        <% else %>
                          Complexité
                        <% end %>
                      </th>
                    <% end %>
                  <% else %>
                    <th>
                      <%= i[0] %>
                    </th>
                  <% end %>
                <% end %>
              <% end %>

              <th class=""><%= I18n.t :actions %></th>
            </tr>

           <%# uowg_guw_unit_of_works.includes(:guw_type, :guw_complexity).order("display_order ASC, name ASC").each_with_index do |guow, i| %>
           <% uowg_guw_unit_of_works.includes(:guw_type, :guw_complexity).each_with_index do |guow, i| %>
              <% @guw_type = guow.guw_type %>
              <% unless @guw_type.nil? %>
                <% @guw_complexities = @guw_type.guw_complexities.order("display_order ASC") %>
              <% end %>

              <tr>
                <%
                   hash_color_priorities = {}

                   gceuws = guow.guw_coefficient_element_unit_of_works
                   gceuws.each do |gceuw|
                     gce = gceuw.guw_coefficient_element
                     unless gce.nil?
                       gce_guw_coefficient = gce.guw_coefficient
                       unless gce_guw_coefficient.nil?
                         unless gce_guw_coefficient.coefficient_type == "Liste" || gce_guw_coefficient.coefficient_type == ""
                           if (gceuw.percent.to_f != gce.value.to_f)
                             hash_color_priorities[gce.color_priority.to_i] = gce.color_code
                           end
                         end
                       end
                     end
                   end


                   @guw_outputs.each do |o|
                     if !guow.size.nil? && !guow.ajusted_size.nil?
                       if guow.size["#{o.id}"].to_f != guow.ajusted_size["#{o.id}"].to_f
                         hash_color_priorities[o.color_priority.to_i] = o.color_code
                       end
                     end
                     if guow.intermediate_percent != guow.intermediate_weight
                       hash_color_priorities[@guw_type.color_priority.to_i] = @guw_type.color_code
                     end
                   end

                   h = Hash[hash_color_priorities.sort.reverse].first
                   unless @guw_type.nil?
                     if @guw_type.allow_line_color == true
                       unless h.nil?
                         c = h.last
                         border_color = "6px solid ##{c};"
                       else
                         border_color = "6px solid aeaeae;"
                       end
                     else
                       border_color = "6px solid aeaeae;"
                     end
                   end
                %>

                <td style="border-left: <%= border_color %>" class="span1">
                  <%= check_box_tag "selected[]", guow.id, guow.selected, class: "selected checkbox_#{uowg.id}", selected: guow.id, disabled: !can?(:execute_estimation_plan, @project) %>
                  <%= hidden_field_tag "", uowg.id, class: "#{guow.id}_group" %>
                  <%= hidden_field_tag "", guow.id, class: "#{guow.id}" %>
                </td>

                <td class="span1">
                  <span class="badge badge-default">
                    #<%= guow.id %>
                  </span>
                </td>

                <td class="text_field_text_overflow span3">
                  <span class="pull-left">
                    <%= link_to guow.name, guw.guw_unit_of_work_load_comments_path(guow), title: guow.name, method: "post", remote: true %>
                  </span>
                </td>

                <td class="span3">
                  <% unless @guw_type.nil? %>
                    <%= select_tag "guw_type[#{guow.id}]", options_for_select(@guw_types.map do |i|
                                                                                default_complexity = i.guw_complexities.where(default_value: true).first
                                                                                ["#{i.name} #{default_complexity.nil? ? nil : '(' + default_complexity.weight.to_s + ')'}", i.id]
                                                                              end,
                                                                              selected: @guw_type.id),
                                   class: "guw_type",
                                   style: "width: 80%",
                                   prompt: "",
                                   guw: guow.id %>
                  <% else %>
                    <%= select_tag "guw_type[#{guow.id}]", options_for_select(@guw_types.map{|i| [i.name, i.id]}),
                                   class: "guw_type",
                                   style: "width: 80%;",
                                   prompt: "",
                                   guw: guow.id %>
                  <% end %>
                </td>

                <% @deported_guw_coefficients = @guw_coefficients.where(deported: true, guw_model_id: @guw_model.id) %>
                <% guow_guw_unit_of_work_attributes = guow.guw_unit_of_work_attributes.where(guw_type_id: @guw_type.id) %>
                <% @guw_model.orders.sort_by { |k, v| v.to_f }.each do |i| %>
                  <% if i[1].blank? %>
                    <% if i[0] == "Coeff. de Complexité" %>
                      <% unless @guw_type.nil? %>
                        <%= select_tag "guw_complexity_#{guow.id}", options_for_select(@guw_complexities.map{|j| [j.name, j.id] },
                                                                                       selected: @guw_complexities.first.nil? ? nil : @guw_complexities.first.id),
                                       guw: guow.id,
                                       class: "guw_complexity hide" %>
                      <% end %>
                    <% end %>
                  <% else %>

                    <% @guw_coefficient = Guw::GuwCoefficient.where(name: i[0], guw_model_id: @guw_model.id).first %>
                    <% @guw_output = Guw::GuwOutput.where(name: i[0], guw_model_id: @guw_model.id).first %>
                    <% if i[0] == "Coefficient" %>
                      <% unless @deported_guw_coefficients.map(&:id).empty? %>
                        <%
                           elmnts = []
                           has_element = false
                           @deported_guw_coefficients.each do |guw_coefficient|
                             ceuw = Guw::GuwCoefficientElementUnitOfWork.where(guw_unit_of_work_id: guow.id,
                                                                               guw_coefficient_id: guw_coefficient.id).first
                             unless ceuw.nil?
                               elm = ceuw.guw_coefficient_element
                               has_element = false
                               unless elm.nil?
                                 has_element = true
                                 elmnts << elm.value
                               end
                             end
                           end
                        %>

                        <td class="span2">
                          <%# if has_element == true %>
                            <% v = elmnts.flatten.compact.inject(&:*) %>
                            <%= link_to v.nil? ? '...' : v.to_f.round(4), guw.deported_path(guw_unit_of_work_id: guow.id), remote: true, method: "POST" %>
                          <%# else %>
                          <%# end %>
                        </td>

                      <% end %>
                    <% elsif i[0] == "Critères" %>
                      <% if @guw_types.map{|i| i.allow_criteria }.flatten.include?(true) %>
                        <td class="span2">
                          <% unless @guw_type.nil? %>
                            <% if @guw_type.allow_criteria == true %>
                              <span class="text-center criteria_label_<%= guow.id %>">
                                <% counting = guow_guw_unit_of_work_attributes.map(&:most_likely).compact %>
                                <% if counting.empty? %>
                                  <%= link_to "", guw.guw_unit_of_work_load_cotations_path(guow), method: "post", remote: true, class: "fa fa-gears #{(guow.missing_value == true) ? 'red' : (guow.guw_complexity_id !=  guow.guw_original_complexity_id ? 'blue' : '') }" %>
                                <% else %>
                                  <%= link_to "", guw.guw_unit_of_work_load_cotations_path(guow), method: "post", remote: true, class: "fa fa-gears #{(guow.missing_value == true) ? 'red' : (guow.guw_complexity_id !=  guow.guw_original_complexity_id ? 'blue' : 'green')}" %>
                                <% end %>
                              </span>
                            <% else %>
                              <span class="text-center">
                                -
                              </span>
                            <% end %>
                          <% end %>
                        </td>
                      <% end %>
                    <% elsif i[0] == "Coeff. de Complexité" %>
                      <td class="span2">
                        <span class="cotation_label_<%= guow.id %>">
                          <% unless @guw_type.nil? %>
                            <% if @guw_type.display_threshold == true %>
                              <% if guow.off_line == true %>
                                <% @default_complexity = @guw_complexities.where(default_value: true).first %>
                                <%= select_tag "guw_complexity_#{guow.id}", options_for_select(@guw_complexities.order("display_order ASC").map{|i| [i.name, i.id] }, selected: nil),
                                               guw: guow.id,
                                               class: "guw_complexity",
                                               prompt: "HSAT",
                                               readonly: !@guw_type.allow_complexity,
                                               style: "width: 80%;" %>
                              <% elsif guow.off_line_uo == true %>
                                <%= select_tag "guw_complexity_#{guow.id}", options_for_select(@guw_complexities.order("display_order ASC").map{|i| [i.name, i.id] }, selected: nil),
                                               guw: guow.id,
                                               class: "guw_complexity",
                                               prompt: "HSUO",
                                               readonly: !@guw_type.allow_complexity,
                                               style: "width: 80%;" %>
                              <% elsif guow.guw_complexity.nil? %>
                                <%= select_tag "guw_complexity_#{guow.id}", options_for_select(@guw_complexities.order("display_order ASC").map{|i| [i.name, i.id] }, selected: @default_complexity.nil? ? guow.guw_complexity_id : @default_complexity.id),
                                               guw: guow.id,
                                               class: "guw_complexity",
                                               include_blank: false,
                                               readonly: !@guw_type.allow_complexity,
                                               style: "width: 80%;" %>
                              <% else %>
                                <%= select_tag "guw_complexity_#{guow.id}", options_for_select(@guw_complexities.order("display_order ASC").map{|i| [i.name, i.id] }, selected: guow.guw_complexity_id), guw: guow.id,
                                               class: "guw_complexity",
                                               include_blank: false,
                                               readonly: !@guw_type.allow_complexity,
                                               style: "width: 80%;" %>
                              <% end %>
                            <% else %>
                              <% if guow.off_line == true %>
                                HSAT
                              <% elsif  guow.off_line_uo == true %>
                                HSUO
                              <% else %>
                                <% if guow.guw_complexity_id == nil %>
                                  -
                                <% else %>
                                  <% if guow.intermediate_percent != guow.intermediate_weight %>
                                    <% color = @guw_type.color_code %>
                                  <% end %>
                                  <%= number_field_tag "complexity_coeff[#{guow.id}]", guow.intermediate_percent,
                                                       style: "width: 80%; text-align: right;",
                                                       class: "complexity_weight-#{uowg.id} guw-#{guow.id}",
                                                       guw: guow.id,
                                                       readonly: true %>
                                  <% if @guw_type.allow_complexity %>
                                    <%= number_field_tag "complexity_coeff_ajusted[#{guow.id}]", guow.intermediate_weight,
                                                         style: "width: 80%; background-color: ##{color}; text-align: right;",
                                                         class: "complexity_weight-#{uowg.id} retained-guw-#{guow.id} retained-gp-#{uowg.id} guw-#{guow.id} retained",
                                                         guw: guow.id,
                                                         title: guow.cplx_comments,
                                                         step: 0.001 %>
                                    <script>
                                      $(document).ready(function(){
                                        var previousSavedValue = $(".retained-guw-" + <%= guow.id %>).val();

                                        $("#complexity_coeff_ajusted_" + <%= guow.id %>).on("change", function(event){

                                          var currentValue = $(this).val();

                                          if(parseFloat(currentValue) != parseFloat(previousSavedValue)) {

                                            $("#guw_form").bind("submit");
                                            $(".guw_submit").prop("disabled", true);

                                            $.ajax({
                                              url: "<%= guw.load_cplx_comments_path %>",
                                              data: "guw_unit_of_work_id=" + $(this).attr("guw") +
                                              "&value=" + currentValue +
                                              "&previousValue=" + parseFloat(previousSavedValue)
                                            });
                                          }
                                        });
                                      });
                                    </script>

                                  <% end %>
                                <% end %>
                              <% end %>
                            <% end %>
                          <% end %>
                        </span>
                      </td>
                    <% elsif @guw_coefficient.class == Guw::GuwCoefficient %>

                      <% unless @guw_coefficient.nil? %>

                        <% unless @guw_type.nil? %>
                          <% results = @guw_coefficient.guw_coefficient_elements.map{|i| i.guw_complexity_coefficient_elements.includes(:guw_coefficient_element).where(guw_type_id: @guw_type.id).select{|ct| ct.value != nil }.map{|i| i.guw_coefficient_element }.uniq }.flatten.compact.sort! { |a, b|  a.display_order.to_i <=> b.display_order.to_i } %>
                        <% else %>
                          <% results = [] %>
                        <% end %>
                        <% default = @guw_coefficient.guw_coefficient_elements.where(default: true).first %>
                        <% if default.nil? %>
                          <% default = results.first %>
                        <% end %>

                        <% if @guw_coefficient.deported == false %>

                          <% unless results.empty? %>

                            <% if @guw_coefficient.coefficient_type == "Pourcentage" || @guw_coefficient.coefficient_type == "Coefficient" %>

                              <% ceuw = Guw::GuwCoefficientElementUnitOfWork.where(guw_unit_of_work_id: guow.id,
                                                                                   guw_coefficient_id: @guw_coefficient.id,
                                                                                   guw_coefficient_element_id: nil).last %>

                              <td class="span2 <%= @guw_coefficient.id %>_<%= guow.id %>">
                                <%
                                   if ceuw.nil?
                                     color = ""
                                   else
                                     if default.value.to_f == ceuw.percent.to_f
                                       color = ""
                                     else
                                       color = "background-color: ##{default.color_code}"
                                     end
                                   end
                                %>
                                <%= number_field_tag "guw_coefficient_percent[#{guow.id}][#{@guw_coefficient.id}]",
                                                     ceuw.nil? ? results.first.value : ceuw.percent.to_f.round(2),
                                                     style: "width: 45%; #{color}; text-align: right;",
                                                     class: "complexity_weight-#{uowg.id} guw-#{guow.id} coeff_#{guow.id}_#{@guw_coefficient.id}_#{results.first.id}",
                                                     readonly: (@guw_coefficient.allow_intermediate_value == true),
                                                     step: 0.001,
                                                     guw: guow.id,
                                                     coeff_id: @guw_coefficient.nil? ? nil : @guw_coefficient.id,
                                                     coeff_element_id: results.first.id,
                                                     title: ceuw.nil? ? "" : ceuw.comments %>

                                <% if @guw_coefficient.allow_intermediate_value == true %>
                                  <%= number_field_tag "guw_coefficient_percent[#{guow.id}][#{@guw_coefficient.id}]", ceuw.nil? ? results.first.value : ceuw.percent.to_f.round(2),
                                                       style: "width: 45%; text-align: right;",
