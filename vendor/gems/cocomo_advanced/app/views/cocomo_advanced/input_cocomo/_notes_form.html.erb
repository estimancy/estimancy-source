<%= form_tag notes_form_path(params[:factor_id]), method: "POST" do %>
    <div class="span12">

      <div class="zdzd">
        <label>Notes</label>
        <%= text_area_tag "notes", @notes, input_html:{ class: "input-large" } %>
      </div>

      <br>

      <%= submit_tag "Save", class: "btn" %>
    </div>
<% end %>