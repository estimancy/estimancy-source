<%= form_tag notes_form_path(params[:factor_id]), method: "POST" do %>
  <div class="span12">

    <div class="zdzd">
      <label><%= I1_n.t(:notes)%></label>
      <%= text_area_tag I1_n.t(:notes), @notes, class: "attribute_note_area" %>
    </div>

    <br>

    <%= submit_tag I18n.t(:save), class: "btn" %>
  </div>
<% end %>